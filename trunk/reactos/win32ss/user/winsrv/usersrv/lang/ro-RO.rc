LANGUAGE LANG_ROMANIAN, SUBLANG_NEUTRAL

IDD_END_NOW DIALOGEX 0, 0, 200, 95
STYLE DS_SHELLFONT | DS_MODALFRAME | DS_CENTER | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Terminare execuție - "
FONT 8, "MS Shell Dlg"
BEGIN
