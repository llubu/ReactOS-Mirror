LANGUAGE LANG_UKRAINIAN, SUBLANG_DEFAULT

100 DIALOG LOADONCALL MOVEABLE DISCARDABLE  0, 0, 220, 62
STYLE DS_MODALFRAME | DS_CENTER | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Завантаження елементу управління Mozilla Active X"
FONT 8, "MS Shell Dlg"
{
 CONTROL "Progress1",1000,"msctls_progress32",WS_BORDER|PBS_SMOOTH,10,10,200,12
 LTEXT "", 104, 10, 30, 200, 10, SS_CENTER
 PUSHBUTTON "Скасувати", IDCANCEL, 85, 44, 50, 15, WS_GROUP | WS_TABSTOP
}

STRINGTABLE
BEGIN
 1001	"Цей додаток потребує об'єкт ActiveX оглядача,\n" \
        "але елемент управління Mozilla Active X не встановлений.\n" \
        "Ви бажаєте завантажити і встановити його??"
END
