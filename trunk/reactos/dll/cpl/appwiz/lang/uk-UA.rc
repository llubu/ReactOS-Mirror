LANGUAGE LANG_UKRAINIAN, SUBLANG_DEFAULT

IDD_PROPPAGEINSTALL DIALOGEX 0, 0, PROPSHEETWIDTH, PROPSHEETHEIGHT
STYLE DS_SETFONT | DS_FIXEDSYS | WS_CHILD | WS_DISABLED | WS_CAPTION
CAPTION "Установити/Видалити"
FONT 8, "MS Shell Dlg", 0, 0, 0x0
BEGIN
  ICON            IDI_CPLSYSTEM,-1,PROPSHEETPADDING,18,16,16
  LTEXT "Щоб установити нову програму, натисніть ""Установити""",-1,36,18,PROPSHEETWIDTH-48,8
  PUSHBUTTON "Ус&тановити...", IDC_INSTALL, PROPSHEETWIDTH-(72), 36, 60, 14
  LTEXT "", -1, PROPSHEETPADDING, 63, PROPSHEETWIDTH-12, 1, NOT WS_GROUP | SS_SUNKEN
  
  ICON            IDI_CPLSYSTEM,-1,PROPSHEETPADDING,76,16,16
  CONTROL "&Показати оновлення",IDC_SHOWUPDATES,"button",BS_AUTOCHECKBOX | WS_TABSTOP,PROPSHEETPADDING, 105, 119, 18
  LTEXT "Нижченаведені &програми можна видалити або переустановити автоматично. Щоб видалити програму або змінити склад установлених компонентів, виберіть цю програму зі списку й натисніть кнопку ""Замінити/Видалити"".",-1,36,72,PROPSHEETWIDTH-48,32
  LISTBOX IDC_SOFTWARELIST, PROPSHEETPADDING, 123, PROPSHEETWIDTH-12, 88, LBS_STANDARD | WS_TABSTOP
  PUSHBUTTON "Замінити/&Видалити", IDC_ADDREMOVE, PROPSHEETWIDTH-72, 198, 60, 14
END

