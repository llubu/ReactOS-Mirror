LANGUAGE LANG_GREEK, SUBLANG_DEFAULT

IDR_POPUP_APP MENU
BEGIN
  POPUP "popup"
  BEGIN
    MENUITEM "Αλλαγή/&Κατάργηση",ID_APP_MODIFYREMOVE
    MENUITEM "&Πληροφορίες",  ID_APP_INFORMATION
  END
END

