	ICON IDI_DOWNLOADICO,-1,PROPSHEETPADDING,80,32,32
	LTEXT "点击“下载”按钮以下载并安装新程序。",-1,36,83,PROPSHEETWIDTH-48,17
	PUSHBUTTON "下载！(&D)", IDC_ADD_DOWNLOAD_BUTTON, PROPSHEETWIDTH-(90), 125, 80, 14, WS_DISABLED
	LTEXT "", -1, PROPSHEETPADDING, 150, PROPSHEETWIDTH-12, 1, NOT WS_GROUP | SS_SUNKEN
	ICON IDI_APPINETICO,-1,PROPSHEETPADDING,160,32,32
	LTEXT "点击“查找Internet”以通过Internet连接查找可用程序。",-1,36,161,PROPSHEETWIDTH-48,17
	PUSHBUTTON "属性(&P)...", IDC_ADD_PROP_BUTTON, PROPSHEETWIDTH-(175), 205, 80, 14, WS_DISABLED
	PUSHBUTTON "查找Internet", IDC_ADD_FIND_BUTTON, PROPSHEETWIDTH-(90), 205, 80, 14, WS_DISABLED
END

