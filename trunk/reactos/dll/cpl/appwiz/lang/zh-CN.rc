IDR_POPUP_APP MENU
BEGIN
  POPUP "popup"
  BEGIN
    MENUITEM "更改/删除(&R)",ID_APP_MODIFYREMOVE
    MENUITEM "信息(&I)",  ID_APP_INFORMATION
  END
END

IDR_POPUP_UPD MENU
BEGIN
  POPUP "popup"
  BEGIN
    MENUITEM "删除(&R)",      ID_UPD_REMOVE
    MENUITEM "信息(&I)", ID_UPD_INFORMATION
  END
END

IDD_PROPPAGEINSTALL DIALOGEX 0, 0, PROPSHEETWIDTH, PROPSHEETHEIGHT
STYLE DS_SHELLFONT | WS_CHILD | WS_DISABLED | WS_CAPTION
CAPTION "更改或删除程序"
FONT 8, "MS Shell Dlg", 0, 0, 0x0
BEGIN
	ICON IDI_CPLSYSTEM,-1,PROPSHEETPADDING,6,16,16
	LTEXT "下列程序可以被自动删除。从下面的列表中选中程序并点击“添加/删除”按钮以删除该程序或修改其组件。",-1,36,7,PROPSHEETWIDTH-48,22
	LTEXT "查找：",-1,PROPSHEETPADDING,33,30,8
	EDITTEXT IDC_FIND_EDIT, PROPSHEETPADDING+30, 30, 180, 14, WS_VISIBLE | WS_TABSTOP
	LTEXT "视图：",-1,250,33,30,8
	COMBOBOX IDC_VIEW_COMBO, 280, 30, 94, 80, CBS_DROPDOWNLIST | WS_CHILD | WS_VISIBLE | WS_TABSTOP | WS_VSCROLL
	CONTROL "", IDC_SOFTWARELIST, "SysListView32", LVS_REPORT | LVS_SORTASCENDING | LVS_AUTOARRANGE | LVS_SINGLESEL, PROPSHEETPADDING, 50, PROPSHEETWIDTH-12, 155, WS_EX_CLIENTEDGE
	PUSHBUTTON "更改/删除(&R)", IDC_ADDREMOVE, PROPSHEETPADDING, 210, 80, 14, WS_DISABLED
	PUSHBUTTON "信息(&I)", IDC_INFO_BUTTON, PROPSHEETPADDING+85, 210, 80, 14, WS_DISABLED
END

