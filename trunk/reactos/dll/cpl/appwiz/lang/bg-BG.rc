IDD_PROPPAGEINSTALL DIALOGEX 0, 0, PROPSHEETWIDTH, PROPSHEETHEIGHT
STYLE DS_SETFONT | DS_FIXEDSYS | WS_CHILD | WS_DISABLED | WS_CAPTION
CAPTION "Промяна или премахване на приложения"
FONT 8, "MS Shell Dlg", 0, 0, 0x0
BEGIN 
	ICON IDI_CPLSYSTEM,-1,PROPSHEETPADDING,6,16,16
	LTEXT "Следното осигуряване може да бъде премахнато. За да премахнете приложение или за да промените сложените съставки, изберете го от списъка и натиснете ""Промяна/ Премахване"".",-1,36,7,PROPSHEETWIDTH-48,32
	LTEXT "Търсене:",-1,PROPSHEETPADDING,33,50,14
	EDITTEXT IDC_FIND_EDIT, PROPSHEETPADDING+30, 30, 180, 14, WS_VISIBLE | WS_TABSTOP
	LTEXT "Изглед:",-1,250,33,40,8
	COMBOBOX IDC_VIEW_COMBO, 280, 30, 94, 80, CBS_DROPDOWNLIST | WS_CHILD | WS_VISIBLE | WS_TABSTOP | WS_VSCROLL
	CONTROL "", IDC_SOFTWARELIST, "SysListView32", LVS_REPORT | LVS_SORTASCENDING | LVS_AUTOARRANGE | LVS_SINGLESEL, PROPSHEETPADDING, 50, PROPSHEETWIDTH-12, 155, WS_EX_CLIENTEDGE
	PUSHBUTTON "Промяна/ Пре&махване", IDC_ADDREMOVE, PROPSHEETPADDING, 210, 80, 14, WS_DISABLED
	PUSHBUTTON "&Сведения", IDC_INFO_BUTTON, PROPSHEETPADDING+85, 210, 80, 14, WS_DISABLED
END

IDD_PROPPAGEUPDATES DIALOGEX 0, 0, PROPSHEETWIDTH, PROPSHEETHEIGHT
STYLE DS_SETFONT | DS_FIXEDSYS | WS_CHILD | WS_DISABLED | WS_CAPTION
CAPTION "Премахване на обновяванията"
FONT 8, "MS Shell Dlg", 0, 0, 0x0
BEGIN
	ICON IDI_UPDATES,-1,PROPSHEETPADDING,6,16,16
	LTEXT "Следните обновявания могат да бъдат премахнати. За да премахнете обновяване, изберете го от списъка и натиснете ""Премахни"".",-1,36,7,PROPSHEETWIDTH-48,32
	LTEXT "Търсене:",-1,PROPSHEETPADDING,33,50,14
	EDITTEXT IDC_FIND_EDIT, PROPSHEETPADDING+30, 30, 180, 14, WS_VISIBLE | WS_TABSTOP
	LTEXT "Изглед",-1,250,33,40,8
	COMBOBOX IDC_VIEW_COMBO, 280, 30, 94, 80, CBS_DROPDOWNLIST | WS_CHILD | WS_VISIBLE | WS_TABSTOP | WS_VSCROLL
	CONTROL "", IDC_SOFTWARELIST, "SysListView32", LVS_REPORT | LVS_SORTASCENDING | LVS_AUTOARRANGE | LVS_SINGLESEL, PROPSHEETPADDING, 50, PROPSHEETWIDTH-12, 155, WS_EX_CLIENTEDGE
	PUSHBUTTON "Пре&махване", IDC_ADDREMOVE, PROPSHEETPADDING, 210, 80, 14, WS_DISABLED
	PUSHBUTTON "&Сведения", IDC_INFO_BUTTON, PROPSHEETPADDING+85, 210, 80, 14, WS_DISABLED
END

IDD_PROPPAGEADD DIALOGEX 0, 0, PROPSHEETWIDTH, PROPSHEETHEIGHT
STYLE DS_SETFONT | DS_FIXEDSYS | WS_CHILD | WS_DISABLED | WS_CAPTION
CAPTION "Добавяне на нови приложения"
FONT 8, "MS Shell Dlg", 0, 0, 0x0
BEGIN
	ICON IDI_CPLSYSTEM,-1,PROPSHEETPADDING,10,32,32
	LTEXT "За да сложите ново приложение, натиснете ""Слагане"".",-1,36,11,PROPSHEETWIDTH-48,8
	PUSHBUTTON "&Слагане...", IDC_INSTALL, PROPSHEETWIDTH-(90), 46, 80, 14
	LTEXT "", -1, PROPSHEETPADDING, 70, PROPSHEETWIDTH-12, 1, NOT WS_GROUP | SS_SUNKEN
	ICON IDI_DOWNLOADICO,-1,PROPSHEETPADDING,80,32,32
	LTEXT "За да свалите и сложите ново приложение, натиснете ""Сваляне"".",-1,36,83,PROPSHEETWIDTH-48,17
	PUSHBUTTON "Сваляне", IDC_ADD_DOWNLOAD_BUTTON, PROPSHEETWIDTH-(90), 125, 80, 14, WS_DISABLED
	LTEXT "", -1, PROPSHEETPADDING, 150, PROPSHEETWIDTH-12, 1, NOT WS_GROUP | SS_SUNKEN
	ICON IDI_APPINETICO,-1,PROPSHEETPADDING,160,32,32
	LTEXT "Натиснете копчето ""Търси в интернет"", за да търсите нови приложения, налични в интернет.",-1,36,161,PROPSHEETWIDTH-48,17
	PUSHBUTTON "&Свойства...", IDC_ADD_PROP_BUTTON, PROPSHEETWIDTH-(175), 205, 80, 14, WS_DISABLED
	PUSHBUTTON "Търсе&не в интернет", IDC_ADD_FIND_BUTTON, PROPSHEETWIDTH-(90), 205, 80, 14, WS_DISABLED
END

