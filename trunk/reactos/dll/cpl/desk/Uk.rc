IDR_POPUP_MENU MENU DISCARDABLE
BEGIN
  POPUP ""
  BEGIN
	MENUITEM "&Конфігурувати", ID_MENU_CONFIG
	MENUITEM "Пере&гляд",      ID_MENU_PREVIEW
	MENUITEM SEPARATOR
	MENUITEM "&Додати",        ID_MENU_ADD
	MENUITEM "В&идалити\tDel", ID_MENU_DELETE
  END
END

IDR_PREVIEW_MENU MENU
BEGIN
    MENUITEM "Normal",           ID_MENU_NORMAL
    MENUITEM "Disabled",         ID_MENU_DISABLED
    MENUITEM "Selected",         ID_MENU_SELECTED
END

