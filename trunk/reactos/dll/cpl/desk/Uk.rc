                    WS_VISIBLE | WS_BORDER, 7, 7, 232, 120
    LTEXT           "Item:", IDC_STATIC, 7, 159, 26, 9
    COMBOBOX        IDC_APPEARANCE_UI_ITEM, 7, 169, 120, 54, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT           "Size:", IDC_STATIC, 142, 159, 16, 9
