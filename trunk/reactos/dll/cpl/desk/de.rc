IDR_POPUP_MENU MENU DISCARDABLE
BEGIN
    POPUP ""
    BEGIN
        MENUITEM "&Konfigurieren",  ID_MENU_CONFIG
        MENUITEM "&Vorschau",       ID_MENU_PREVIEW
        MENUITEM SEPARATOR
        MENUITEM "&Hinzufügen",     ID_MENU_ADD
        MENUITEM "&Löschen\tDel",   ID_MENU_DELETE
    END
END

IDR_PREVIEW_MENU MENU
