IDR_POPUP_MENU MENU DISCARDABLE
BEGIN
  POPUP ""
  BEGIN
	MENUITEM "&Configurer",     ID_MENU_CONFIG
	MENUITEM "A&perçu",       ID_MENU_PREVIEW
	MENUITEM SEPARATOR
	MENUITEM "&Ajouer",           ID_MENU_ADD
	MENUITEM "Supprimer\tSuppr",   ID_MENU_DELETE
  END
END

