IDR_POPUP_MENU MENU DISCARDABLE
BEGIN
  POPUP ""
  BEGIN
	MENUITEM "&Configure",     ID_MENU_CONFIG
	MENUITEM "&Preview",       ID_MENU_PREVIEW
	MENUITEM SEPARATOR
	MENUITEM "&Add",           ID_MENU_ADD
	MENUITEM "&Delete\tDel",   ID_MENU_DELETE
  END
END

