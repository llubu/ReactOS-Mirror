IDD_TIMEPAGE DIALOGEX 0, 0, 246, 188
STYLE DS_SETFONT | DS_FIXEDSYS | WS_CHILD | WS_DISABLED | WS_CAPTION
CAPTION "Час"
FONT 8, "MS Shell Dlg"
BEGIN
  GROUPBOX "Зразок вигляду", -1, 7, 7, 228, 33, WS_CHILD | WS_VISIBLE
  LTEXT "Зразок:", -1, 13, 21, 54, 10, WS_CHILD |WS_VISIBLE | WS_GROUP
  EDITTEXT IDC_TIMESAMPLE, 68, 19, 84, 14, ES_READONLY | WS_CHILD | WS_VISIBLE | WS_GROUP
  LTEXT "&Формат часу:", -1, 13, 52, 54, 10, WS_CHILD |WS_VISIBLE | WS_GROUP
  COMBOBOX IDC_TIMEFORMAT, 68, 50, 84, 100, CBS_DROPDOWN | WS_CHILD | WS_VISIBLE | WS_TABSTOP
  LTEXT "&Розділювач компонентів часу:", -1, 13, 70, 54, 10, WS_VISIBLE | WS_GROUP
  COMBOBOX IDC_TIMESEPARATOR, 68, 68, 84, 100, CBS_DROPDOWN | WS_CHILD | WS_VISIBLE | WS_TABSTOP
  LTEXT "По&значення часу до полудня (AM):", -1, 13, 88, 54, 10, WS_VISIBLE | WS_GROUP
  COMBOBOX IDC_TIMEAMSYMBOL, 68, 86, 84, 100, CBS_DROPDOWN | WS_CHILD | WS_VISIBLE | WS_TABSTOP
  LTEXT "&Позначення часу після полудня (PM):", -1, 13, 106, 54, 10, WS_VISIBLE | WS_GROUP
  COMBOBOX IDC_TIMEPMSYMBOL, 68, 104, 84, 100, CBS_DROPDOWN | WS_CHILD | WS_VISIBLE | WS_TABSTOP
  GROUPBOX "", -1, 7, 124, 228, 60, WS_VISIBLE
  LTEXT "Позначення формату часу\n\n\
h = година     m = хвилина     s = секунда     t = АМ або РМ\n\n\
h = 12-годинний формат доби   H = 24-годинний формат доби\n\
hh, mm, ss = з виводом нулів на початку   h, m, s = без виводу нулів",
        -1, 13, 133, 214, 47, WS_CHILD |WS_VISIBLE | WS_GROUP
END


