IDD_TIMEPAGE DIALOGEX 0, 0, 246, 188
STYLE DS_SETFONT | DS_FIXEDSYS | WS_CHILD | WS_DISABLED | WS_CAPTION
CAPTION "Время"
FONT 8, "MS Shell Dlg"
BEGIN
  GROUPBOX "Оформление", -1, 7, 7, 228, 33, WS_CHILD | WS_VISIBLE
  LTEXT "Образец:", -1, 10, 21, 54, 10, WS_CHILD |WS_VISIBLE | WS_GROUP
