FONT 8, "MS Shell Dlg"
BEGIN
    ICON IDI_CPLICON, IDI_CPLICON, 8,5,32,32
    LTEXT "",-1,40,10,194,36
    GROUPBOX "Gerätelautstärke", -1, 7,30,230,100
    ICON IDI_CPLICON, IDI_CPLICON, 17,45,32,32
