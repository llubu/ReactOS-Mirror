  GROUPBOX "Автоматический сброс",-1,PROPSHEETPADDING(1),LABELLINE(1),PROPSHEETWIDTH-PROPSHEETPADDING(2),LABELLINE(5)
  CHECKBOX "&Отключать при простое в течение:",IDC_RESET_BOX,PROPSHEETPADDING(2),LABELLINE(2)-2,PROPSHEETWIDTH-PROPSHEETPADDING(6)-ICONSIZE,LABELLINE(2)

  //POPUP IDC_RESET_POPUP, GRAYED
  //{
  //  MENUITEM "5 Минут", IDC_RESET_5
