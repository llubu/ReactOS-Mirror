    LTEXT "Exemples", IDC_STATIC, 14, 73, 70, 10
    LTEXT "Nombre :", IDC_STATIC, 16, 86, 48, 10
    LTEXT "Symbole monétaire :", IDC_STATIC, 16, 101, 48, 10
    LTEXT "Heure :", IDC_STATIC, 16, 116, 48, 10
