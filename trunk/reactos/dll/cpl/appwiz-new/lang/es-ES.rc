FONT 8, "MS Shell Dlg"
BEGIN
    CONTROL "", IDB_WATERMARK, "STATIC", SS_BITMAP | SS_REALSIZEIMAGE, 0, 0, WATERMARK_WIDTH, SHORTCUTDLG_HEIGHT
    LTEXT "Este asistente le ayudará a crear accesos directos a programas locales o en red, archivos, carpetas, computadoras, o direcciones de Internet..", -1, 97, 7, 195, 28
