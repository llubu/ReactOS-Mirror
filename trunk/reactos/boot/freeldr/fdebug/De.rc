    PUSHBUTTON      "&Durchsuchen",IDC_BROWSE,194,26,50,14
    DEFPUSHBUTTON   "OK",IDOK,139,74,50,14
    PUSHBUTTON      "Abbrechen",IDCANCEL,194,74,50,14
END


