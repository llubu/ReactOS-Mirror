        MENUITEM SEPARATOR
        MENUITEM "Zakoñ&cz",                CMD_EXIT
    END
    POPUP "&Edycja"
    BEGIN
        MENUITEM "&Cofnij\tCtrl+Z",           CMD_UNDO
        MENUITEM SEPARATOR
        MENUITEM "&Wytnij\tCtrl+X",           CMD_CUT
