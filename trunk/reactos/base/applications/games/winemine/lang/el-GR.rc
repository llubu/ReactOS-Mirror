IDM_WINEMINE MENU DISCARDABLE
BEGIN
	POPUP "&Game"
	BEGIN
		MENUITEM "&Νέο\tF2",                 IDM_NEW
		MENUITEM SEPARATOR
		MENUITEM "&Σημείωση με ερωτηματικό", IDM_MARKQ
		MENUITEM SEPARATOR
		MENUITEM "&Αρχάριος",                IDM_BEGINNER
		MENUITEM "&Προχωρημένος",            IDM_ADVANCED
		MENUITEM "&Ειδικός",                 IDM_EXPERT
		MENUITEM "&Προσωπικό",               IDM_CUSTOM
		MENUITEM SEPARATOR
		MENUITEM "&Γρηγορότεροι Χρόνοι",     IDM_TIMES
		MENUITEM SEPARATOR
		MENUITEM "Έ&ξοδος",                  IDM_EXIT
	END
	POPUP "&?"
	BEGIN
		MENUITEM "&Σχετικά...",              IDM_ABOUT
	END
END

