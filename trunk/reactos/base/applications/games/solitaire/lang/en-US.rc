#include "resource.h"

#define APSTUDIO_READONLY_SYMBOLS

/* winemaker: #include "afxres.h" */
/* winemaker:warning: 'afxres.h' is an MFC specific header. Replacing it with 'winres.h' */
#include "winres.h"

