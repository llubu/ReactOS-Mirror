    PUSHBUTTON "&Löschen", ID_TAB_DEL, 72, 80, 45, 15
    PUSHBUTTON "&OK", IDOK, 137, 15, 50, 15
    PUSHBUTTON "A&bbrechen", IDCANCEL, 137, 33, 50, 15
    PUSHBUTTON "&Alle löschen", ID_TAB_EMPTY, 137, 51, 50, 15
END

