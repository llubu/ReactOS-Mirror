LANGUAGE LANG_GREEK, SUBLANG_DEFAULT

/* Menu */

MAIN_MENU MENU
BEGIN
 POPUP "&Αρχείο"
 BEGIN
  MENUITEM "&’νοιγμα", MNID_FILE_OPEN
  MENUITEM SEPARATOR
  MENUITEM "&Εκτύπωση", MNID_FILE_PRINT
  MENUITEM "Ρυ&θμίσεις εκτυπωτή...", MNID_FILE_SETUP
   MENUITEM SEPARATOR
  MENUITEM "Έ&ξοδος", MNID_FILE_EXIT
 END
 POPUP "&ΕΠεξεργασία"
 BEGIN
   MENUITEM "&Αντιγραφή...", MNID_EDIT_COPYDLG
   MENUITEM SEPARATOR
   MENUITEM "&Annotate...", MNID_EDIT_ANNOTATE
 END
 POPUP "Α&γαπημένα"
 BEGIN
   MENUITEM "&Define...", MNID_BKMK_DEFINE
 END
 POPUP "&Βοήθεια"
 BEGIN
   MENUITEM "Help &on help", MNID_HELP_HELPON
   MENUITEM "Πάντα στο προ&σκήνιο", MNID_HELP_HELPTOP
   MENUITEM SEPARATOR
   MENUITEM "&Πληροφορίες...", MNID_HELP_ABOUT
#ifdef WINELIB
   MENUITEM "&Σχετικά με το WINE", MNID_HELP_WINE
#endif
 END
END

/* Strings */

