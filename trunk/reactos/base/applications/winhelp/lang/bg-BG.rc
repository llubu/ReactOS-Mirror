/* Menu */

MAIN_MENU MENU
{
 POPUP "&Файл"
  {
  MENUITEM "&Отваряне", MNID_FILE_OPEN
  MENUITEM SEPARATOR
  MENUITEM "&Печат", MNID_FILE_PRINT
  MENUITEM "&Настройки на печатача...", MNID_FILE_SETUP
  MENUITEM SEPARATOR
