   MENUITEM "&Сведения...", MNID_HELP_ABOUT
#ifdef WINELIB
   MENUITEM "&За...", MNID_HELP_WINE
#endif
 }
}

IDD_INDEX DIALOG DISCARDABLE 0, 0, 200, 190 LANGUAGE LANG_BULGARIAN, SUBLANG_DEFAULT
STYLE DS_MODALFRAME | WS_CAPTION | WS_SYSMENU
