        MENUITEM "Exit",                        ID_EXIT
    END
    MENUITEM "todo1",                       0
    MENUITEM "todo2",                       0
    POPUP "?"
    BEGIN
        MENUITEM "About...",                    ID__ABOUT
    END
END

