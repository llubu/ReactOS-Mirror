MAIN_MENU MENU DISCARDABLE
{
   POPUP "&Правка" {
      MENUITEM "&Копіювати Ctrl+C",      IDM_COPY
      MENUITEM "Вст&авити Ctrl+V",       IDM_PASTE
   }
