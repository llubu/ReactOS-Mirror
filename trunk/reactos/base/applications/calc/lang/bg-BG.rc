IDR_MENU_SCIENTIFIC_1 MENU DISCARDABLE 
BEGIN
    POPUP "&Обработка"
    BEGIN
        MENUITEM "Запомняне\tCTRL-C",           IDM_EDIT_COPY
        MENUITEM "Поставяне\tCTRL-V",           IDM_EDIT_PASTE
    END
    POPUP "Изглед"
    BEGIN
        MENUITEM "Обикновен",                   IDM_VIEW_STANDARD, CHECKED
