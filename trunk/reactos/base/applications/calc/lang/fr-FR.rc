      MENUITEM "Standard",         IDM_MODE_STANDARD
      MENUITEM "Scientifique",       IDM_MODE_SCIENTIFIC
      MENUITEM SEPARATOR
      MENUITEM "Séparateur",   IDM_DIGIT_GROUPING
   }
   POPUP "Ai&de" {
      MENUITEM "Somaire",      IDM_HELP_TOPICS
      MENUITEM SEPARATOR
      MENUITEM "A propos", IDM_ABOUT
   }
}

