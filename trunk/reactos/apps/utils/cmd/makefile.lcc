TARGET=cmd
CC=lcc.exe
CFLAGS=-c -O
LD=lcclnk.exe
LFLAGS=-subsystem console -s -o $(TARGET).exe
OBJS=alias.obj \
	attrib.obj \
	batch.obj \
	beep.obj \
	call.obj \
	chcp.obj \
	cls.obj \
	cmd.obj \
	cmdinput.obj \
	cmdtable.obj \
	color.obj \
	console.obj \
	copy.obj \
	date.obj \
	del.obj \
	dir.obj \
	dirstack.obj \
	echo.obj \
	error.obj \
	filecomp.obj \
	for.obj \
	goto.obj \
	history.obj \
	if.obj \
	internal.obj \
	label.obj \
	locale.obj \
	misc.obj \
	move.obj \
	path.obj \
	pause.obj \
	prompt.obj \
	redir.obj \
	ren.obj \
	set.obj \
	shift.obj \
	start.obj \
	time.obj \
	type.obj \
	ver.obj \
	verify.obj \
	vol.obj \
	where.obj 

# MAIN

.c.obj:
	$(CC) $(CFLAGS) $<

$(TARGET).exe: $(OBJS)
	$(LD) $(LFLAGS) $(OBJS)

#EOF
