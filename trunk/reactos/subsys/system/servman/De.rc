    MENUITEM "E&xit",ID_FILE_EXIT
  END
  POPUP "Action"
  BEGIN
    MENUITEM "Start",ID_START
    MENUITEM "Stopp",ID_STOP
    MENUITEM "Pause",ID_PAUSE
