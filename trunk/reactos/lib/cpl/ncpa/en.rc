 CONTROL "",IDC_SUBNETMASK,"SysIPAddress32",0,150,90-2,80,12
 LTEXT "Default gateway:", -1, 14, 105, 135, 8
 CONTROL "",IDC_DEFGATEWAY,"SysIPAddress32",0,150,105-2,80,12
