 CONTROL "",IDC_DNS2,"SysIPAddress32",0,150,186-2,80,12
 PUSHBUTTON "&Advanced", IDC_ADVANCED, 186, 209, 50, 14, WS_DISABLED | WS_TABSTOP
}


