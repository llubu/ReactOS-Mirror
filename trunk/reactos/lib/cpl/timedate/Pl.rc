            WS_CHILD | WS_VISIBLE | WS_TABSTOP,
            11, 37, 108, 80
    GROUPBOX "&Godzina", -1, 132, 2, 113, 125
    CONTROL "", IDC_TIMEPICKER, "SysDateTimePick32",
            DTS_TIMEFORMAT | WS_CHILD | WS_VISIBLE | WS_TABSTOP,
            144, 17, 90, 12
