compile: $(JSCS)

jsas: $(JSS)
	rm -f $@.js
	for i in $(JSS); do cat $(srcdir)/$$i >> $@.js; done
	$(JSCOMPILER) $(JSCOMPILER_FLAGS) -c $@.js
	echo "#!$(bindir)/js --file" > $@
#	echo "#!$(JSCOMPILER) --file" > $@
	cat $@.jsc >> $@
	chmod a+x $@

.js.jsc:
	$(JSCOMPILER) $(JSCOMPILER_FLAGS) -c $<

